@model List<WebProje.Models.WorkingTimes>
<div class="block full">
    <div class="block-title">
        <h2>Data</h2>
    </div>
    <div class="table-responsive">
        <div id="example-datatable_wrapper" class="Data_wrapper form-inline no-footer">
            <table id="example-datatable" class="table table-striped table-bordered table-vcenter dataTable no-footer" role="grid" aria-describedby="example-datatable_info">
                <thead>
                <tr role="row">
                    <th class="text-center sorting_asc" style="width: 49px;" tabindex="0" aria-controls="example-datatable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="ID: activate to sort column descending">NO</th>
                    <th class="sorting" tabindex="0" aria-controls="example-datatable" rowspan="1" colspan="1" aria-label="User: activate to sort column ascending" style="width: 191px;">Days</th>
                    <th class="sorting" tabindex="0" aria-controls="example-datatable" rowspan="1" colspan="1" aria-label="User: activate to sort column ascending" style="width: 191px;">Time</th>
                    <th class="text-center sorting_disabled" style="width: 74px;" rowspan="1" colspan="1" aria-label="">
                        <i class="fa fa-flash"></i>
                    </th>
                </tr>
                </thead>
                <tbody>
                @{ int i = 1; }
                @foreach (WorkingTimes wt in Model)
                {
                    <tr role="row" class="odd">
                        <td class="text-center sorting_1">@(i++)</td>
                        <td>
                            <strong>@wt.DaysOfWeek</strong>
                        </td>
                        <td>
                            <strong>@(wt.StartTime + " - " + wt.EndTime)</strong>
                        </td>
                        <td class="text-center">
                            <a href="/Doctors/DeleteWorkingTimes/@wt.Id" data-toggle="tooltip" title="" class="btn btn-effect-ripple btn-xs btn-danger" style="overflow: hidden; position: relative;" data-original-title="Delete User">
                                x
                            </a>
                        </td>
                    </tr>
                }
                </tbody>
            </table>
        </div>
    </div>
</div>